<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.motorhome.controller.rental.popup.RentalExtraSelectionController">
   <children>
      <HBox alignment="CENTER" prefHeight="40.0" prefWidth="500.0" spacing="150.0" styleClass="newEntityBar" HBox.hgrow="ALWAYS">
         <children>
            <Label fx:id="title" text="Pick Extras" />
         </children>
      </HBox>
      <ScrollPane hbarPolicy="NEVER" prefHeight="200.0" styleClass="edge-to-edge" VBox.vgrow="ALWAYS">
         <content>
            <VBox fx:id="extrasContainer" alignment="TOP_CENTER" prefWidth="600.0" spacing="5.0" />
         </content>
      </ScrollPane>
   </children>
</VBox>
